{
  "metadata": {
    "title": "Grumpydale"
  },
  "source": "src/pages",
  "destination": "dist",
  "plugins": {
    "metalsmith-assets": {
      "source": "src/assets_root",
      "destination": "."
    },
    "metalsmith-assets": {
      "source": "src/assets_css",
      "destination": "./css"
    },
    "metalsmith-assets": {
      "source": "src/assets_js",
      "destination": "./js"
    },
    "metalsmith-assets": {
      "source": "src/assets_img",
      "destination": "./img"
    },
    "metalsmith-markdown": {
      "smartypants": true,
      "gfm": true,
      "tables": true
    },
    "metalsmith-layouts": {
      "engine": "handlebars",
      "directory": "src/templates",
      "partials": "src/partials"
    },
    "metalsmith-headings": true
  }
}
